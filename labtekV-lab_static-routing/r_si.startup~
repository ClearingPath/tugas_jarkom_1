ifconfig eth0 192.168.12.1 up
ifconfig eth1 100.0.2.2 netmask 255.255.255.252 broadcast 100.0.2.3 up
ifconfig eth2 100.0.3.1 netmask 255.255.255.252 broadcast 100.0.3.3 up
route add -net 192.168.16.0 netmask 255.255.252.0 gw 100.0.3.2 dev eth2
route add -net 192.168.10.0 netmask 255.255.254.0 gw 100.0.2.1 dev eth1
route add -net 192.168.0.0 netmask 255.255.248.0 gw 100.0.3.2 dev eth2
